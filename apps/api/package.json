{
  "name": "@execution-os/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "seed": "set -a && . ../../.env && set +a && tsx src/seed.ts",
    "prisma:generate": "set -a && . ../../.env && set +a && prisma generate",
    "prisma:migrate": "set -a && . ../../.env && set +a && prisma migrate dev --name init"
  },
  "dependencies": {
    "@execution-os/shared": "0.1.0",
    "@prisma/client": "^5.20.0",
    "amqplib": "^0.10.4",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "@fastify/cors": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "prisma": "^5.20.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
